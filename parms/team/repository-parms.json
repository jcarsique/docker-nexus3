{
  "repositories": {
    "maven2": {
      "hosted": {
        "maven-public": {
          "version_policy": "MIXED"
        },
        "maven-private": {
          "version_policy": "MIXED"
        }
      },
      "proxy": {
        "maven-upstream": {
          "blobstore": {
            "name": "store",
            "strict_content_type_validation": "false"
          },
          "remote": {
            "url": "https://packages.nuxeo.com/repository/maven-internal/",
            "auth": "true"
          },
          "version_policy": "MIXED"
        }
      },
      "group": {
        "maven-group": {
          "repositories": [
            "maven-public",
            "maven-private",
            "maven-upstream"
          ]
        }
      }
    },
    "npm": {
      "hosted": {
        "npm-public": {
        },
        "npm-private": {
        }
      },
      "proxy": {
        "npm-upstream": {
          "blobstore": {
            "name": "store",
            "strict_content_type_validation": "true"
          },
          "remote": {
            "url": "https://packages.nuxeo.com/repository/npm-internal/",
            "auth": "true"
          }
        }
      }
    }
  },
  "passwords": {
    "file": [
      "/opt/sonatype/nexus/config/passwords.json"
    ]
  },
  "remotes": {
    "packages.nuxeo.com": {
      "auth_type": "username",
      "user": "deployment"
    }
  },
  "blobstores": {
    "name": "store"
  }
}
